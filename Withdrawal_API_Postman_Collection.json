{
	"info": {
		"_postman_id": "withdrawal-api-collection",
		"name": "Withdrawal API",
		"description": "Complete API collection for wallet withdrawal functionality",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"auth": {
		"type": "bearer",
		"bearer": [
			{
				"key": "token",
				"value": "{{auth_token}}",
				"type": "string"
			}
		]
	},
	"item": [
		{
			"name": "Get Available Balance",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{base_url}}/api/withdrawal/available-balance",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"withdrawal",
						"available-balance"
					]
				},
				"description": "Get the available balance for withdrawal (investor profile only)"
			},
			"response": []
		},
		{
			"name": "Get Banks List",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{base_url}}/api/withdrawal/banks",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"withdrawal",
						"banks"
					]
				},
				"description": "Get list of all active Saudi banks from database"
			},
			"response": []
		},
		{
			"name": "Get Saved Bank Accounts",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{base_url}}/api/withdrawal/bank-accounts",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"withdrawal",
						"bank-accounts"
					]
				},
				"description": "Get all saved bank accounts for the authenticated user"
			},
			"response": []
		},
		{
			"name": "Add Bank Account (Save for Future)",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"bank_id\": 1,\n    \"account_holder_name\": \"أحمد محمد علي\",\n    \"iban\": \"SA0380000000608010167519\",\n    \"account_number\": \"1234\",\n    \"save_for_future\": true,\n    \"set_as_default\": true\n}"
				},
				"url": {
					"raw": "{{base_url}}/api/withdrawal/bank-accounts",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"withdrawal",
						"bank-accounts"
					]
				},
				"description": "Add a new bank account and save it for future use"
			},
			"response": []
		},
		{
			"name": "Add Bank Account (Validate Only)",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"bank_id\": 1,\n    \"account_holder_name\": \"أحمد محمد علي\",\n    \"iban\": \"SA0380000000608010167519\",\n    \"save_for_future\": false\n}"
				},
				"url": {
					"raw": "{{base_url}}/api/withdrawal/bank-accounts",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"withdrawal",
						"bank-accounts"
					]
				},
				"description": "Validate bank account without saving (will be saved with withdrawal request)"
			},
			"response": []
		},
		{
			"name": "Create Withdrawal Request (Using Saved Account)",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"amount\": 5000.00,\n    \"bank_account_id\": 1\n}"
				},
				"url": {
					"raw": "{{base_url}}/api/withdrawal/request",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"withdrawal",
						"request"
					]
				},
				"description": "Create withdrawal request using a saved bank account"
			},
			"response": []
		},
		{
			"name": "Create Withdrawal Request (New Bank Details)",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"amount\": 5000.00,\n    \"bank_id\": 1,\n    \"account_holder_name\": \"أحمد محمد علي\",\n    \"iban\": \"SA0380000000608010167519\"\n}"
				},
				"url": {
					"raw": "{{base_url}}/api/withdrawal/request",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"withdrawal",
						"request"
					]
				},
				"description": "Create withdrawal request with new bank account details (not saved)"
			},
			"response": []
		},
		{
			"name": "Get Withdrawal History",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{base_url}}/api/withdrawal/history?per_page=15&status=pending",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"withdrawal",
						"history"
					],
					"query": [
						{
							"key": "per_page",
							"value": "15",
							"description": "Items per page (default: 15)"
						},
						{
							"key": "status",
							"value": "pending",
							"description": "Filter by status: pending, processing, completed, rejected, cancelled",
							"disabled": true
						}
					]
				},
				"description": "Get withdrawal request history with optional status filter"
			},
			"response": []
		}
	],
	"variable": [
		{
			"key": "base_url",
			"value": "https://slide.osta-app.com",
			"type": "string"
		},
		{
			"key": "auth_token",
			"value": "",
			"type": "string"
		}
	]
}


